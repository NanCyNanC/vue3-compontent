{
  "name": "my-vue3-ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/my-vue3-ui.umd.js",
  "module": "./dist/my-vue3-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-vue3-ui.es.js",
      "require": "./dist/my-vue3-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:docs": "vite build --config vite.docs.config.ts",
    "preview": "vite preview",
    "docs": "vite --config vite.docs.config.ts"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.15",
    "@vueuse/core": "^10.4.1",
    "prismjs": "^1.29.0",
    "@types/prismjs": "^1.26.1"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  }
}